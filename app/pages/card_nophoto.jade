extends /blocks/layout-default/layout-default
include ../blocks/breadcrumbs/breadcrumbs
include ../blocks/card/card_nophoto
include ../blocks/card-list/card-list
include ../blocks/block/block
include ../blocks/headblock/headblock
include ../blocks/pagination/pagination
include ../blocks/yad/yad
include ../blocks/tags/tags


block head
	- var pageTitle = 'Купить Mercedes-Benz E-klasse V (W213, S213, C238) в Москве';
	- var pageMod = 'card';

block content
	+breadcrumbs(getData('card-crumbs'))
	+card_nophoto(getData('cars'), 1)
	+headblock
	+card-list(getData('cars'))
	+pagination
	+yad
	+tags
